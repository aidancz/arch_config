# man zathura zathurarc

set recolor true
# set recolor-darkcolor  "#9e9e9e"
# set recolor-lightcolor "#24272b"
set recolor-darkcolor  "#616161"
set recolor-lightcolor "#d4d4d4"

map g goto top
map e scroll down
map y scroll up
map d scroll half-down
map u scroll half-up
map f navigate next
map b navigate previous

map <C-a> toggle_page_mode
map <F2> quit
